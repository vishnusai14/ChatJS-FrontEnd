{
  "expo": {
    "name": "ChatJS",
    "slug": "Chat-JS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chat2icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vishnu.chatjs",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.vishnu.chatjs",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/chat2icon.png",
        "backgroundColor": "#FFFFFF"
      },
        "useNextNotificationsApi" : true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
